#set($layout='/WEB-INF/vm/layout/adminLayout.vm')
        
	<script type="text/javascript">
		require(['calendar'],function(calendar){
			
				jQuery("#view_user").find(".j_datetime_picker").calendar({
						frondose : true,
						disabled: false
					});
				
		});

	</script>
	
    


<div class="modal-body auto-height-body" id="view_user">
		<form class="form-horizontal"  id="form1">
    	
    		 <div class="control-group">
    			<label class="control-label" for="id">id</label>
    			<div class="controls">
    			
					<input type="text"  name="id" id="id" value="$!{user.id}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="userId">账户名</label>
    			<div class="controls">
    			
					<input type="text"  name="userId" id="userId" value="$!{user.userId}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="passwd">密码</label>
    			<div class="controls">
    			
					<input type="text"  name="passwd" id="passwd" value="$!{user.passwd}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="userCode">用户code</label>
    			<div class="controls">
    			
					<input type="text"  name="userCode" id="userCode" value="$!{user.userCode}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="nikeName">别名</label>
    			<div class="controls">
    			
					<input type="text"  name="nikeName" id="nikeName" value="$!{user.nikeName}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="roleType">角色</label>
    			<div class="controls">
    			
        
		<select class="forms_select input-medium form-control" name="roleType" id="roleType" disabled>
						<option value="1" #if($user.roleType==1) selected  #end>用户</option>
				<option value="2" #if($user.roleType==2) selected  #end>管理员</option>

		</select>
	
    

			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="authorityLevel">权限等级</label>
    			<div class="controls">
    			
        
		<select class="forms_select input-medium form-control" name="authorityLevel" id="authorityLevel" disabled>
						<option value="1" #if($user.authorityLevel==1) selected  #end>默认</option>
				<option value="2" #if($user.authorityLevel==2) selected  #end>其他</option>

		</select>
	
    

			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="parentId">父ID</label>
    			<div class="controls">
    			
					<input type="text"  name="parentId" id="parentId" value="$!{user.parentId}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="activateCode">激活码</label>
    			<div class="controls">
    			
					<input type="text"  name="activateCode" id="activateCode" value="$!{user.activateCode}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="userState">用户状态</label>
    			<div class="controls">
    			
        
		<select class="forms_select input-medium form-control" name="userState" id="userState" disabled>
						<option value="0" #if($user.userState==0) selected  #end>初始状态</option>
				<option value="1" #if($user.userState==1) selected  #end>正常</option>
				<option value="2" #if($user.userState==2) selected  #end>欠费</option>
				<option value="3" #if($user.userState==3) selected  #end>不可用</option>

		</select>
	
    

			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="totalLoginTimes">登录次数</label>
    			<div class="controls">
    			
					<input type="text"  name="totalLoginTimes" id="totalLoginTimes" value="$!{user.totalLoginTimes}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="currentClass">当前班级</label>
    			<div class="controls">
    			
					<input type="text"  name="currentClass" id="currentClass" value="$!{user.currentClass}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="currentTeacher">当前老师</label>
    			<div class="controls">
    			
					<input type="text"  name="currentTeacher" id="currentTeacher" value="$!{user.currentTeacher}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="level">等级</label>
    			<div class="controls">
    			
        
		<select class="forms_select input-medium form-control" name="level" id="level" disabled>
						<option value="1" #if($user.level==1) selected  #end>庶人</option>
				<option value="2" #if($user.level==2) selected  #end>童生</option>
				<option value="3" #if($user.level==3) selected  #end>秀才</option>
				<option value="4" #if($user.level==4) selected  #end>举人</option>
				<option value="5" #if($user.level==5) selected  #end>进士</option>
				<option value="6" #if($user.level==6) selected  #end>探花</option>
				<option value="7" #if($user.level==7) selected  #end>榜眼</option>
				<option value="8" #if($user.level==8) selected  #end>状元</option>

		</select>
	
    

			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="cashPoint">积分</label>
    			<div class="controls">
    			
					<input type="text"  name="cashPoint" id="cashPoint" value="$!{user.cashPoint}" readonly/>
				
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="state">状态</label>
    			<div class="controls">
    			
        
		<select class="forms_select input-medium form-control" name="state" id="state" disabled>
						<option value="1" #if($user.state==1) selected  #end>有效</option>
				<option value="2" #if($user.state==2) selected  #end>无效</option>

		</select>
	
    

			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="created">创建时间</label>
    			<div class="controls">
    			
						<input type="text"  name="created" id="created" value="$!dateFormatUtils.format($!user.created)" readonly/>
					
			</div>
		</div>
		
    		 <div class="control-group">
    			<label class="control-label" for="modified">修改时间</label>
    			<div class="controls">
    			
						<input type="text"  name="modified" id="modified" value="$!dateFormatUtils.format($!user.modified)" readonly/>
					
			</div>
		</div>
		

    	</form>
</div>
<div class="modal-footer">
	<a href="javascript:;" class="btn"  data-dismiss="modal" aria-hidden="true">关闭</a>
</div>

