

<div class="manage-content">
<form class="form-inline">


		<label>ID</label>
			
			<input type="text"  name="id" id="id_search" class="input" placeholder="请输入ID">
			

		
		<label>课程ID</label>
			
			<input type="text"  name="lessonId" id="lessonId_search" class="input" placeholder="请输入课程ID">
			

		
		<label>试卷类型</label>
			
	
		<select class="forms_select" name="type" id="type_search">
			<option value="">全部</option>
			
				<option value="1">单元测试</option>
				<option value="2">自主测试</option>
		</select>
	
	


		
		<label>题目类型</label>
			
			<input type="text"  name="questonType" id="questonType_search" class="input" placeholder="请输入题目类型">
			

		
		<label>状态</label>
			
	
		<select class="forms_select" name="state" id="state_search">
			<option value="">全部</option>
			
				<option value="1">有效</option>
				<option value="2">无效</option>
		</select>
	
	


		
		<input type="button" onclick="queryList();" class="btn" value="搜索"  />

	</form>
		
<br/>
		 <div class="page_operate">
			 <button class="btn  btn-primary" type="button" id="add-paper">创建 Paper</button>
		</div>


	<div id="dl"></div>
</div>
<script language="javascript">
	function queryList(pageNo) {	
		if (!pageNo) {
			pageNo = 1;
		}
		var page = pageNo;
		var url = '/paper/list.action';
		var param = {
			page: page
		};
		
		var pageSize=jQuery("#JPageSize_queryList").val();
        if(pageSize && !isNaN(pageSize)){
            param['pageSize']=pageSize;
        }

		getOtherParams(param);
		gUtils.fGetHtml(url, 'dl', param,'数据加载中...');
	}
	queryList();
	

	function getOtherParams(param){
   
	  	var id_search=jQuery("#id_search").val();
		if(id_search!=null&&id_search!=''){
				param.id=id_search;
			}
			 
	  	var lessonId_search=jQuery("#lessonId_search").val();
		if(lessonId_search!=null&&lessonId_search!=''){
				param.lessonId=lessonId_search;
			}
			 
	  	var type_search=jQuery("#type_search").val();
		if(type_search!=null&&type_search!=''){
				param.type=type_search;
			}
			 
	  	var questonType_search=jQuery("#questonType_search").val();
		if(questonType_search!=null&&questonType_search!=''){
				param.questonType=questonType_search;
			}
			 
	  	var state_search=jQuery("#state_search").val();
		if(state_search!=null&&state_search!=''){
				param.state=state_search;
			}
					
	}




void function(j) {
	jQuery("#add-paper").click(function(){
		gDialog.fCreate({title: '创建 Paper', url: '/paper/add.action',width:700}).show();
	});
	
}(jQuery);

</script>